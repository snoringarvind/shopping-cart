iv>\n  );\n};\n\nexport default Cart;\n","/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/Home.js",[],"/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/Card.js",["60"],"import React, { useContext, useState } from \"react\";\nimport { CartContext } from \"./CartContext\";\n\nconst Card = ({ card }) => {\n  const { cart } = useContext(CartContext);\n  const [cartValue, setCartValue] = cart;\n  // const [quantityValue, setQuantityValue] = quantity;\n\n  const addCartHandler = () => {\n    // setQuantityValue(quantityValue + 1);\n    const duplicate = cartValue.some((item, i) => {\n      if (item.label === card.label) {\n        let newArr = [...cartValue];\n        newArr[i].quantity = Number(newArr[i].quantity) + 1;\n        setCartValue(newArr);\n        // console.log(newArr);\n      }\n      return item.label === card.label;\n    });\n\n    if (!duplicate) {\n      const cartItems = { ...card, quantity: 1 };\n      setCartValue((prev) => [...prev, cartItems]);\n    }\n  };\n\n  return (\n    <div className=\"Card\">\n      <div className=\"images\">\n        <img src={card.src} alt={card.src} />\n      </div>\n      <div className=\"description\">\n        <div className=\"label\">{card.label}</div>\n        <div className=\"price\">Rs. {card.price}</div>\n        <div className=\"add-cart-button\">\n          <button\n            type=\"button\"\n            onClick={addCartHandler}\n            style={{ cursor: \"pointer\" }}\n          >\n            Add to cart\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Card;\n","/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/CartContext.js",["61","62"],"/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/CartItem.js",[],"/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/Checkout.js",[],"/home/arvind/Documents/React-apps/the-odin/shopping-cart/src/components/Pay.js",[]